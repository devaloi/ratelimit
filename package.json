{
  "name": "@devaloi/ratelimit",
  "version": "1.0.0",
  "description": "A production-grade Express rate limiting middleware with multiple algorithms (fixed window, sliding window, token bucket) and pluggable backends (memory, Redis).",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts' && prettier --check 'src/**/*.ts' 'tests/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' 'tests/**/*.ts' --fix && prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "rate-limit",
    "rate-limiting",
    "express",
    "middleware",
    "token-bucket",
    "sliding-window",
    "fixed-window",
    "redis",
    "api",
    "throttle"
  ],
  "author": "devaloi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/devaloi/ratelimit.git"
  },
  "homepage": "https://github.com/devaloi/ratelimit#readme",
  "bugs": {
    "url": "https://github.com/devaloi/ratelimit/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "ioredis": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
